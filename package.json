{
  "name": "@wrangle/cli",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "tsx src/index.ts",
    "lint": "biome check .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prepare": "husky"
  },
  "dependencies": {
    "commander": "~12.0.0",
    "zod": "~3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@swc/core": "~1.4.2",
    "@swc/jest": "~0.2.36",
    "@tsconfig/recommended": "^1.0.3",
    "@types/node": "~20.11.25",
    "husky": "^9.0.11",
    "jest": "~29.7.0",
    "jsonschema": "~1.4.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "biome check --apply",
      "prettier --write"
    ]
  }
}
